{
	"name": "ftp-filecontrol",
	"displayName": "FTP-FileControl",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
        "Other"
	],
	"activationEvents": [
        "onCommand:extension.helloWorld",
        "workspaceContains:.vscode/ftp-filecontrol.json"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
                "title": "Hello World"

            },
            {
                "command": "liveworkspace.reload",
                "title": "Reload",
                "icon": {
                    "dark": "./src/media/darkReload.svg",
                    "light": "./src/media/lightReload.svg"
                }
            },
            {
                "command": "liveworkspace.checkout",
                "title": "Check Out"
            },
            {
                "command": "liveworkspace.checkin",
                "title": "Check In"
            }
        ],
        "menus": {
            "commandPalette": [

            ],
            "view/title": [
                {
                    "command": "liveworkspace.reload",
                    "when": "view == live-workspace-remote",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "live-workspace",
                    "title": "Live-Workspace",
                    "icon": "./src/media/laptop.svg"
                }
            ]
        },
        "views": {
            "live-workspace": [
                {
                    "id": "live-workspace-remote",
                    "name": "Remote"
                }
            ]
        },
        "jsonValidation": [
            {
                "fileMatch": ".vscode/sftp.json",
                "url": "./schema/live-workspace.schema.json"
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.33"
	},
	"dependencies": {
		"@types/ftp": "^0.3.29",
		"@types/ssh2-sftp-client": "^2.4.1",
		"ftp": "^0.3.10",
		"ssh2-sftp-client": "^2.5.0"
	}
}
